{
    "id": "base-ubuntu",
    "version": "1.0.0",
    "name": "An ubuntu base image",
    "description": "A base template for qms' devcontainer",
    "documentationURL": "https://github.com/meastp/devcontainer-templates/tree/main/src/base-ubuntu",
    "licenseURL": "https://github.com/meastp/devcontainer-templates/blob/main/LICENSE",
    "options": {
        "imageVariant": {
            "type": "string",
